		{
		    if (GetPlayerWeapon(playerid))
		    {
		        SetPlayerArmedWeapon(playerid, 0);
		    }
		}
	}

	if (t_TriggerFeature[TRIGGER_ZONE_NAMES])
	{
	    new
	        Float:x,
	        Float:y,
			Float:z
		;
	    GetPlayerPos(playerid, x, y, z);

	    if (t_PlayerLastPos[playerid][0] != x || t_PlayerLastPos[playerid][1] != y || t_PlayerLastPos[playerid][2] != z)
	    {
	        t_PlayerLastPos[playerid][0] = x;
			t_PlayerLastPos[playerid][1] = y;
			t_PlayerLastPos[playerid][2] = z;

			new
			    lastcity = t_PlayerLastGPS[playerid][0],
				city[45]
			;
		    GetPlayerCity(playerid, city);

			if (t_PlayerLastGPS[playerid][0] != lastcity)
			{
				if (t_PlayerZoneTxtFade[playerid][fadeTimer] != -1)
				{
					KillTimer(t_PlayerZoneTxtFade[playerid][fadeTimer]);
				}

				t_PlayerZoneTxtFade[playerid][fadeLevel] = 0;

				PlayerTextDrawColor(playerid, t_PlayerZoneTxt[playerid], _setaplha(0xA9C4E4FF, 0));
				PlayerTextDrawSetString(playerid, t_PlayerZoneTxt[playerid], city);
				PlayerTextDrawShow(playerid, t_PlayerZoneTxt[playerid]);

				t_PlayerZoneTxtFade[playerid][fadeTimer] = SetTimerEx("OnZoneFadeUpdate", 30, true, "ii", playerid, 0);
			}
			else
			{
			    new
				    lastzone = t_PlayerLastGPS[playerid][1],
					zone[45]
				;
			    GetPlayerCity(playerid, zone);

				if (t_PlayerLastGPS[playerid][1] != lastzone)
				{
					if (t_PlayerZoneTxtFade[playerid][fadeTimer] != -1)
					{
						KillTimer(t_PlayerZoneTxtFade[playerid][fadeTimer]);
					}

					t_PlayerZoneTxtFade[playerid][fadeLevel] = 0;

					PlayerTextDrawColor(playerid, t_PlayerZoneTxt[playerid], _setaplha(0xA9C4E4FF, 0));
					PlayerTextDrawSetString(playerid, t_PlayerZoneTxt[playerid], zone);
					PlayerTextDrawShow(playerid, t_PlayerZoneTxt[playerid]);

					t_PlayerZoneTxtFade[playerid][fadeTimer] = SetTimerEx("OnZoneFadeUpdate", 30, true, "ii", playerid, 4);
