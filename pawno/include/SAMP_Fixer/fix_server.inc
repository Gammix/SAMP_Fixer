	        KillTimer(t_PlayerVehicleTxtFade[playerid][fadeTimer]);
			t_PlayerVehicleTxtFade[playerid][fadeTimer] = SetTimerEx("OnVehicleFadeUpdate", 30, true, "iii", playerid, 3);
	    }
	    case 4:
	    {
	        t_PlayerVehicleTxtFade[playerid][fadeLevel] -= 15;
			PlayerTextDrawColor(playerid, t_PlayerVehicleTxt[playerid], _setaplha(0x006400FF, t_PlayerVehicleTxtFade[playerid][fadeLevel]));

			if (t_PlayerVehicleTxtFade[playerid][fadeLevel] <= 0)
			{
				PlayerTextDrawHide(playerid, t_PlayerVehicleTxt[playerid]);

				KillTimer(t_PlayerVehicleTxtFade[playerid][fadeTimer]);
				t_PlayerVehicleTxtFade[playerid][fadeTimer] = -1;
			}
			else
			{
				PlayerTextDrawShow(playerid, t_PlayerVehicleTxt[playerid]);
			}
	    }
	}
}

public OnPlayerDisconnect(playerid, reason)
{
	t_PlayerZoneTxt[playerid] = PlayerText:INVALID_TEXT_DRAW;
	if (t_PlayerZoneTxtFade[playerid][fadeTimer] != -1)
	{
		KillTimer(t_PlayerZoneTxtFade[playerid][fadeTimer]);
		t_PlayerZoneTxtFade[playerid][fadeTimer] = -1;
	}

	t_PlayerVehicleTxt[playerid] = PlayerText:INVALID_TEXT_DRAW;
	if (t_PlayerVehicleTxtFade[playerid][fadeTimer] != -1)
	{
		KillTimer(t_PlayerVehicleTxtFade[playerid][fadeTimer]);
		t_PlayerVehicleTxtFade[playerid][fadeTimer] = -1;
	}

	#if defined S_OnPlayerDisconnect
        return S_OnPlayerDisconnect(playerid, reason);
    #else
    	return 1;
	#endif
}
