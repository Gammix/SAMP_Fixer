stock static GetPlayerZone(playerid, zone[])
{
	new
	    Float:pos[3]
	;
	GetPlayerPos(playerid, pos[0], pos[1], pos[2]);

    format(zone, 45, "Gangton");

	for(new i, j = sizeof(t_SAZones); i < j; i++)
	{
	    if ((pos[0] >= t_SAZones[i][zoneMinX] && pos[0] < t_SAZones[i][zoneMaxX]) && (pos[1] >= t_SAZones[i][zoneMinY] && pos[1] < t_SAZones[i][zoneMaxY]))
		{
		    if (zone[0])
			{
				format(zone, 45, t_SAZones[i][zoneName]);
			}

		    t_PlayerLastGPS[playerid][1] = i;

		    break;
		}
	}
}

