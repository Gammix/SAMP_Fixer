	    {
	        t_PlayerZoneTxtFade[playerid][fadeLevel] += 15;
			PlayerTextDrawColor(playerid, t_PlayerZoneTxt[playerid], _setaplha(0xA9C4E4FF, t_PlayerZoneTxtFade[playerid][fadeLevel]));

			if (t_PlayerZoneTxtFade[playerid][fadeLevel] >= 255)
			{
				KillTimer(t_PlayerZoneTxtFade[playerid][fadeTimer]);
				OnZoneFadeUpdate(playerid, 1);
			}
			PlayerTextDrawShow(playerid, t_PlayerZoneTxt[playerid]);
	    }
	    case 1:
	    {
			t_PlayerZoneTxtFade[playerid][fadeTimer] = SetTimerEx("OnZoneFadeUpdate", 3500, false, "iii", playerid, 2);
	    }
	    case 2:
	    {
	        KillTimer(t_PlayerZoneTxtFade[playerid][fadeTimer]);
			t_PlayerZoneTxtFade[playerid][fadeTimer] = SetTimerEx("OnZoneFadeUpdate", 30, true, "iii", playerid, 3);
