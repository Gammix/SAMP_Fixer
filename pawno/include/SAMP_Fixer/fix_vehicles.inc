public OnVehicleMod(playerid, vehicleid, componentid)
{
    new
		modelid = GetVehicleModel(vehicleid)
	;
	if (400 <= modelid <= 611)
	{
		switch (componentid)
		{
			case 1000 .. 1191:
			{
				if (t_VehicleMods[(modelid - 400) * 6 + (componentid - 1000 >>> 5)] & 1 << (componentid - 1000 & 0b00011111))
				{
					#if defined VEH_OnVehicleMod
						return VEH_OnVehicleMod(playerid, vehicleid, componentid);
					#else
						return 1;
					#endif
				}
			}
			case 1192, 1193:
			{
				if (modelid == 576)
				{
					#if defined VEH_OnVehicleMod
						return VEH_OnVehicleMod(playerid, vehicleid, componentid);
					#else
						return 1;
					#endif
				}
			}
		}

		return 0;
	}

