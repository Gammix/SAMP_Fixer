stock VEH_CreateVehicle(modelid, Float:x, Float:y, Float:z, Float:rotation, color1, color2, respawn_delay = 0, addsiren = 0)
{
	if (! (400 <= modelid <= 611))
	{
	    return INVALID_VEHICLE_ID;
	}

	if (color1 == -1 && color2 == -1 && t_VehicleColors[(modelid - 400)][0])
	{
    	if (t_VehicleColors[(modelid - 400)][0] == 2)
		{
		    color1 = t_VehicleColors[(modelid - 400)][1];
		    color2 = t_VehicleColors[(modelid - 400)][2];
		}
		else
		{
			new
				index = random(t_VehicleColors[(modelid - 400)][0]) & -2
			;

			color1 = t_VehicleColors[(modelid - 400)][++index];
			color2 = t_VehicleColors[(modelid - 400)][++index];
		}
	}

	new
		vehicleid = INVALID_VEHICLE_ID
	;
	switch (modelid)
	{
		case 569, 570, 537, 538:
		{
			vehicleid = AddStaticVehicleEx(modelid, x, y, z, rotation, color1, color2, respawn_delay, addsiren);
		}
		default:
		{
			vehicleid = CreateVehicle(modelid, x, y, z, rotation, color1, color2, respawn_delay, addsiren);
		}
	}

	if (vehicleid == INVALID_VEHICLE_ID)
	{
	    return INVALID_VEHICLE_ID;
	}

	#if defined SAMP_FIXER_SINGLES
        t_VehicleData[vehicleid][0] = 0;

        t_VehicleData[vehicleid][1] = color1;
        t_VehicleData[vehicleid][2] = color2;

        t_VehicleData[vehicleid][3] = 3;
	#else
		new
		    buf[40]
		;
		format(buf, sizeof(buf), "vehicle%i_interiorid", vehicleid);
		SetSVarInt(buf, 0);

		format(buf, sizeof(buf), "vehicle%i_color1", vehicleid);
		SetSVarInt(buf, color1);
		format(buf, sizeof(buf), "vehicle%i_color2", vehicleid);
		SetSVarInt(buf, color2);

		format(buf, sizeof(buf), "vehicle%i_paintjob", vehicleid);
		SetSVarInt(buf, 3);
	#endif

	return vehicleid;
}
#if defined _ALS_CreateVehicle
	#undef CreateVehicle
#else
	native BAD_CreateVehicle(modelid, Float:x, Float:y, Float:z, Float:a, color1, color2, respawn_delay, addsiren = 0) = CreateVehicle;
	#define _ALS_CreateVehicle
#endif
#define CreateVehicle VEH_CreateVehicle

